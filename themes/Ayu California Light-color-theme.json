      "settings": {
        "foreground": "#55b4d4"
      }
    },
    {
      "name": "Lambda arrow",
      "scope": ["storage.type.function"],
      "settings": {
        "foreground": "#fa8d3e"
      }
    },
    {
      "name": "Java primitive variable types",
      "scope": ["source.java storage.type.primitive"],
      "settings": {
        "foreground": "#55b4d4"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#f2ae49"
      }
    },
    {
      "name": "Function arguments",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#a37acc"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "variable.function",
