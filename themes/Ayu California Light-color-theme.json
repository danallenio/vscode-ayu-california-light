  "tokenColors": [
    {
      "settings": {
        "background": "#fafafa",
        "foreground": "#6c7680"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#abb0b6"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#86b300"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": ["string.regexp", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#4cbf99"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ff9940"
      }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#ff9940"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#6c7680"
      }
    },
    {
      "name": "Member Variable",
      "scope": ["variable.member"],
      "settings": {
        "foreground": "#f07171"
      }
    },
    {
      "name": "Language variable",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#55b4d4"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#fa8d3e"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#fa8d3e"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ed9366"
      }
    },
    {
      "name": "Separators like ; or ,",
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#6c7680b3"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.section"],
      "settings": {
        "foreground": "#6c7680"
      }
    },
    {
      "name": "Accessor",
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#ed9366"
      }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
        "source.haskell storage.type",
        "source.c storage.type"
      ],
      "settings": {
        "foreground": "#399ee6"
      }
    },
    {
      "name": "Inherited class type",
